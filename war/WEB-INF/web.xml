<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Ajax_jquery_example</servlet-name>
		<servlet-class>org.blanco.jqueryajax.Ajax_jquery_exampleServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Ajax_jquery_example</servlet-name>
		<url-pattern>/ajax_jquery_example</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Post_Example</servlet-name>
		<servlet-class>org.blanco.jqueryajax.PostServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Post_Example</servlet-name>
		<url-pattern>/postexample</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
